{
  "expo": {
    "name": "AurumX",
    "slug": "aurumx-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "aurumx",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aurumx.mobile",
      "buildNumber": "1",
      "icon": "./assets/icon.png",
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "aurumx",
              "com.aurumx.mobile"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "aurumx",
              "com.aurumx.mobile"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.aurumx.mobile",
      "permissions": [
        "android.permission.VIBRATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "aurumx"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.7.22"
          }
        }
      ],
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#D4AF37",
          "defaultChannel": "default"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#D4AF37"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c5dc4f76-463d-4db0-800e-59af980d8ef6"
      }
    },
    "owner": "amitmahajan78"
  }
}
